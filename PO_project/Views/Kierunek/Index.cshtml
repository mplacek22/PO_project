    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Abbreviation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jezyk.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stopien.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tryb.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CzasTrwania.CzasTrwaniaId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.KierunekId">Edit</a> |
